<Window x:Class="CATS.exportProgressScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CATS"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Background="#252525"
        Foreground="#FFFFFF"
        Title="Export - CATS" Height="160" Width="450" MinHeight="160" MinWidth="450" MaxHeight="160" MaxWidth="450">
    <StackPanel Margin="20">
        <TextBlock x:Name="exportTb" FontFamily="Arial" FontSize="14"/>
        <ProgressBar x:Name="exportProg" Height="20" Value="0" Margin="0,20,0,20" ValueChanged="exportProg_ValueChanged"/>
        <TextBlock x:Name="exportStatusTb" FontFamily="Arial" FontSize="12" TextAlignment="Right"/>
    </StackPanel>
</Window>
