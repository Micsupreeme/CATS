<Page x:Class="CATS.ExportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CATS"
      mc:Ignorable="d" 
            d:DesignHeight="450" d:DesignWidth="770"
      Background="#252525"
      Foreground="#FFFFFF"
      Title="ExportPage">
    <StackPanel Margin="30">
        <Separator Margin="0,30,0,30"/>
        <TextBlock x:Name="exportTb" FontFamily="Arial" FontSize="14" Text="Export Coursework Assignment Brief:"/>
        <Button x:Name="exportPdfBtn" FontFamily="Arial" FontSize="14" Content="Export to PDF (with Appendicies)" Padding="27,3,27,3" Margin="20" Click="exportPdfBtn_Click"/>
        <Button x:Name="exportPdfBtn1" FontFamily="Arial" FontSize="14" Content="test image compress" Padding="27,3,27,3" Margin="20" Click="exportPdfBtn1_Click"/>
    </StackPanel>
</Page>
